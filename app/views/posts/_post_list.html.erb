<% @posts.each do |post| %>
  <dl class="dl-horizontal list-group">
      <dt>Original url:</dt>
      <dd><%= post.base_url %></dd>
      <dt>Shorten url:</dt>
      <dd onclick ="openUrl('<%=new_url(post.short_url)%>');" class="shortern">
          <%=new_url(post.short_url)%>
      </dd>
      <dt>Total clicks</dt>
      <dd><%=post.count%></dd>
      <dt>Top countries:</dt>
      <dd><%= get_countries(post.id) %></dd>
  </dl>
<%end%>
